{% trans_default_domain dgo.tdFix %}

{% import '@HBMDatagrid/macros.html.twig' as dgm %}

<thead {{ dgo.parseTableHeadAttr()|raw }}>
  <tr>
    {% for column, cell in dgo.cells %}
      {% if (cell.isVisibleNormal and not dgo.extended) or (cell.isVisibleExtended and dgo.extended) %}
        <th {{ cell.attr('th', null, column, null)|raw }}>
          {% include '@HBMDatagrid/Datagrid/table-head-th.html.twig' with { 'dgo': dgo, 'cell': cell, 'column': column } only %}
        </th>
      {% endif %}
    {% endfor %}
  </tr>
</thead>

