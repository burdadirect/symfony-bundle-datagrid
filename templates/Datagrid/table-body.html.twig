{% trans_default_domain 'HBMDatagridBundle' %}

<tbody {{ datagrid.parseTableBodyAttr()|raw }}>
{% for row, obj in datagrid.results %}
  <tr {{ datagrid.parseRowAttr(obj, row)|raw }}>
    {% for column, cell in datagrid.cells %}
      {% if (cell.isVisibleNormal and not datagrid.extended) or (cell.isVisibleExtended and datagrid.extended) %}
        <td {{ cell.attr('td')|raw }}>
          {% include cell.template(obj, column, row) with cell.templateParams(obj, column, row)|merge({ 'datagrid': datagrid, 'cell': cell, 'column': column, 'row': row, 'obj': obj, 'transDomain': transDomain }) only %}
        </td>
      {% endif %}
    {% endfor %}
  </tr>
{% endfor %}
</tbody>

