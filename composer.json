{
  "name": "burdanews/symfony-bundle-datagrid",
  "description": "HBM Datagrid Bundle - List, filter, sort and paginate (query builder) results.",
  "authors" : [{
    "name" : "Christian Puchinger",
    "email" : "christian.puchinger@burda.com"
  }],
  "license": "MIT",
  "type": "symfony-bundle",
  "autoload": {
    "psr-0": {
      "HBM\\DatagridBundle": ""
    }
  },
  "repositories" : [
    {
      "type": "vcs",
      "url": "git@github.com:burdanews/symfony-bundle-twig-attributes.git"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "burdanews/symfony-bundle-twig-attributes": "dev-master",
    "symfony/config": "^3.4|^4.2",
    "symfony/routing": "^3.4|^4.2",
    "symfony/http-foundation": "^3.4|^4.2",
    "symfony/http-kernel": "^3.4|^4.2",
    "symfony/dependency-injection": "^3.4|^4.2",
    "doctrine/orm": "^2.5",
    "psr/log": "^1.0",
    "phpoffice/phpspreadsheet": "^1.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "target-dir": "HBM/DatagridBundle"
}
